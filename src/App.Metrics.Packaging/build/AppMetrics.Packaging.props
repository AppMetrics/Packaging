<Project>

  <PropertyGroup>
    <Product>App Metrics</Product>
    <Authors Condition="'$(Authors)' == ''">App Metrics Contributors</Authors>
    <Company Condition="'$(Company)' == ''">App Metrics</Company>
    <Copyright Condition="'$(Copyright)' == ''">© $([System.DateTime]::UtcNow.ToString(yyyy)) App Metrics.</Copyright>
    <PackageTags Condition="'$(PackageTags)' == ''">appmetrics;metrics</PackageTags>
    <PackageIconUrl Condition="'$(PackageIconUrl)' == ''">https://app-metrics.io/images/logo.png</PackageIconUrl>
    <PackageOutputPath Condition="'$(PackageOutputPath)' == ''">..\..\.artifacts\packages</PackageOutputPath>
    <PackageProjectUrl Condition="'$(PackageProjectUrl)' == ''">https://app-metrics.io/</PackageProjectUrl>
    <PackageLicenseUrl Condition="'$(PackageLicenseUrl)' == ''">https://github.com/alhardy/AppMetrics/blob/master/LICENSE</PackageLicenseUrl>
    <GeneratePackageOnBuild Condition="'$(GeneratePackageOnBuild)' == ''">false</GeneratePackageOnBuild>
    <GenerateDocumentationFile Condition="'$(GenerateDocumentationFile)' == ''">true</GenerateDocumentationFile>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
		<GenerateNeutralResourcesLanguageAttribute>false</GenerateNeutralResourcesLanguageAttribute>
		<GenerateAssemblyCopyrightAttribute>false</GenerateAssemblyCopyrightAttribute>
    <NoWarn>$(NoWarn);</NoWarn>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

</Project>